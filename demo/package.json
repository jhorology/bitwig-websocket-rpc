{
  "name": "bitwig-websocket-rpc-demo",
  "version": "0.1.0",
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=4"
  },
  "keywords": [
    "grommet",
    "nextjs",
    "react"
  ],
  "scripts": {
    "dev": "next dev -p 1234",
    "alias": "NODE_ENV=alias next dev -p 1234",
