    "babel-plugin-transform-imports": "^1.5.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-parallel": "^0.3.2",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "pre-commit": "^1.2.2"
